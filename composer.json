{
    "name": "raveesh/laravel-folder-report",
    "description": "A Laravel package to generate folder size reports with HTML output.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Raveesh\\FolderReport\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raveesh\\FolderReport\\FolderReportServiceProvider"
            ]
        }
    }
}